{
  "author": "Jaco Greeff <jacogr@gmail.com>",
  "bugs": "https://github.com/polkadot-js/phishing/issues",
  "homepage": "https://github.com/polkadot-js/phishing#readme",
  "license": "Apache-2.0",
  "packageManager": "yarn@3.2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/polkadot-js/phishing.git"
  },
  "sideEffects": false,
  "version": "0.17.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "polkadot-dev-build-ts",
    "build:extra": "node scripts/sortAll.mjs",
    "build:release": "polkadot-ci-ghact-build",
    "build:release:ipfs": "node scripts/ipfsUpload.mjs",
    "build:rollup": "polkadot-exec-rollup --config",
    "clean": "polkadot-dev-clean-build",
    "deno": "yarn polkadot-dev-deno-map && yarn build && deno check --import-map=import_map.json mod.ts",
    "lint": "polkadot-dev-run-lint",
    "phishing:addrcheck": "ava packages/phishing/src/addrcheck.spec.ts",
    "phishing:crosscheck": "ava packages/phishing/src/crosscheck.spec.ts",
    "postinstall": "polkadot-dev-yarn-only",
    "test": "ava --match '!CI*'",
    "test:one": "yarn test"
  },
  "dependencies": {
    "@pinata/sdk": "^1.1.26",
    "dnslink-cloudflare": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@polkadot/dev": "^0.67.63",
    "ava": "^4.3.0",
    "ts-node": "^10.8.2"
  },
  "resolutions": {
    "typescript": "^4.7.4"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm",
      "--experimental-specifier-resolution=node"
    ]
  }
}
