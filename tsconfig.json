{
  "extends": "@polkadot/dev/config/tsconfig.json",
  "exclude": [
    "build/**/*",
    "**/build/**/*"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths":
    {
      "@polkadot/phishing": ["packages/phishing/src"],
      "@polkadot/phishing/*": ["packages/phishing/src/*"]
    },
    "skipLibCheck": true,
    "typeRoots": [
      "./node_modules/@types"
    ]
  }
}
